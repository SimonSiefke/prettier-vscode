{
  "name": "prettier-vscode",
  "displayName": "Prettier - Code formatter",
  "description": "Code formatter using prettier",
  "publisher": "SimonSiefke",
  "version": "1.0.1",
  "galleryBanner": {
    "color": "#ffe04b",
    "theme": "light"
  },
  "engines": {
    "vscode": "^1.41.1"
  },
  "categories": [
    "Programming Languages",
    "Formatters"
  ],
  "keywords": [
    "multi-root ready"
  ],
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "activationEvents": [
    "onLanguage:csharp",
    "onLanguage:css",
    "onLanguage:elm",
    "onLanguage:flow",
    "onLanguage:graphql",
    "onLanguage:handlebars",
    "onLanguage:html",
    "onLanguage:java",
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:json",
    "onLanguage:jsonc",
    "onLanguage:less",
    "onLanguage:markdown",
    "onLanguage:mdx",
    "onLanguage:php",
    "onLanguage:postcss",
    "onLanguage:ruby",
    "onLanguage:scss",
    "onLanguage:solidity",
    "onLanguage:svelte",
    "onLanguage:svg",
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onLanguage:twig",
    "onLanguage:vue",
    "onLanguage:xml",
    "onLanguage:yaml"
  ],
  "homepage": "https://github.com/SimonSiefke/prettier-vscode",
  "repository": {
    "type": "git",
    "url": "https://github.com/SimonSiefke/prettier-vscode.git"
  },
  "bugs": {
    "url": "https://github.com/SimonSiefke/prettier-vscode/issues"
  },
  "icon": "icon.png",
  "main": "./dist/extensionMain.js",
  "contributes": {
    "jsonValidation": [
      {
        "fileMatch": ".prettierrc",
        "url": "http://json.schemastore.org/prettierrc"
      },
      {
        "fileMatch": ".prettierrc.json",
        "url": "http://json.schemastore.org/prettierrc"
      },
      {
        "fileMatch": "package.json",
        "url": "./package-json-schema.json"
      }
    ],
    "languages": [
      {
        "id": "json",
        "filenames": [
          ".prettierrc"
        ]
      },
      {
        "id": "ignore",
        "filenames": [
          ".prettierignore"
        ]
      },
      {
        "id": "vue",
        "extensions": [
          ".vue"
        ]
      },
      {
        "id": "elm",
        "extensions": [
          ".elm"
        ]
      },
      {
        "id": "graphql",
        "extensions": [
          ".gql"
        ]
      },
      {
        "id": "java",
        "extensions": [
          ".java"
        ]
      },
      {
        "id": "mdx",
        "extensions": [
          ".mdx"
        ]
      },
      {
        "id": "ruby",
        "extensions": [
          ".rb"
        ]
      },
      {
        "id": "solidity",
        "extensions": [
          ".sol"
        ]
      },
      {
        "id": "svelte",
        "extensions": [
          ".svelte"
        ]
      },
      {
        "id": "twig",
        "extensions": [
          ".twig"
        ]
      },
      {
        "id": "yaml",
        "extensions": [
          ".yml"
        ]
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Prettier configuration",
      "properties": {
        "prettier.enabledLanguageIds": {
          "description": "Languages for which formatting with Prettier should be enabled",
          "type": "array",
          "uniqueItems": true,
          "items": {
            "type": "string",
            "enum": [
              "csharp",
              "css",
              "elm",
              "flow",
              "graphql",
              "handlebars",
              "html",
              "java",
              "javascript",
              "javascriptreact",
              "json",
              "jsonc",
              "less",
              "markdown",
              "mdx",
              "php",
              "postcss",
              "ruby",
              "scss",
              "solidity",
              "svelte",
              "svg",
              "typescript",
              "typescriptreact",
              "twig",
              "vue",
              "xml",
              "yaml"
            ]
          },
          "scope": "window",
          "default": [
            "csharp",
            "css",
            "elm",
            "flow",
            "graphql",
            "handlebars",
            "html",
            "java",
            "javascript",
            "javascriptreact",
            "json",
            "jsonc",
            "less",
            "markdown",
            "mdx",
            "php",
            "postcss",
            "ruby",
            "scss",
            "solidity",
            "svelte",
            "svg",
            "typescript",
            "typescriptreact",
            "twig",
            "vue",
            "xml",
            "yaml"
          ]
        }
      }
    }
  },
  "devDependencies": {
    "@types/vscode": "^1.41.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "source-map-support": "^0.5.16",
    "vscode-languageclient": "^6.1.0"
  }
}
